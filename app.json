{
  "expo": {
    "name": "OrderKu",
    "scheme": "myapp",
    "slug": "OrderKu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconOK.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashOK.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7E813"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconOK.png",
        "backgroundColor": "#F7E813"
      },
      "package": "com.hafidzbandung.OrderKU"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "f6d188e5-9eed-4711-bd3c-7cf36f077e2b"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
