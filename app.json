{
  "expo": {
    "name": "OrderKu",
    "scheme": "myapp",
    "slug": "OrderKu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconOK.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashOK.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7E813"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconOK.png",
        "backgroundColor": "#F7E813",
      },
      "package": "com.hafidzbandung.OrderKU"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "4eb3ea62-779f-4e8c-b3f7-b44b8d848861"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
